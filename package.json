{
  "name": "mindful-web-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "compose-up": "docker compose -f deploy/docker-compose.yml build mwf-build-image && docker compose -f deploy/docker-compose.yml up -d --build",
    "prepare": "husky",
    "start": "vite --config web/vite.config.ts",
    "build": "tsc -p web/tsconfig.app.json --noEmit && vite build --config web/vite.config.ts",
    "lint": "eslint web --config web/eslint.config.js",
    "lint:fix": "eslint web --config web/eslint.config.js --fix",
    "type-check": "tsc -p web/tsconfig.app.json --noEmit",
    "test": "vitest run --config web/vitest.config.ts",
    "test:coverage": "vitest run --config web/vitest.config.ts --coverage",
    "preview": "vite preview --config web/vite.config.ts"
  },
  "lint-staged": {
    "web/**/*.{js,jsx,ts,tsx}": [
      "eslint --config web/eslint.config.js --fix"
    ]
  },
  "dependencies": {
    "@types/node": "^25.0.10",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.555.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.6",
    "recharts": "^3.5.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vitest": "^3.2.4"
  }
}
